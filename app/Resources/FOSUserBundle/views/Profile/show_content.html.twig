{% trans_default_domain 'FOSUserBundle' %}

<table class="table table-dark">
    <thead>
        <tr>
            <th>{{ 'profile.show.username'|trans }}</th>
            <th>{{ 'profile.show.email'|trans }}</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>{{ user.username }}</td>
            <td>{{ user.email }}</td>
        </tr>
    </tbody>
</table>
<a href="{{ path('fos_user_profile_edit') }}" class="btn btn-outline-light">{{ 'profile.edit.submit'|trans }}</a>
